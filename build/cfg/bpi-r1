X_BUILD_BASE_CFG="armv6"
KERNCONF=ALLWINNER

# The configuration load/save script needs to know the
# configuration partition name and location.
BIN_CFG_PARTITION="/dev/map/cfg"
BIN_CFG_SIZE="65536"

UBOOT_ARCH="armv6"
UBOOT_KERN_LOADADDR="0x80200000"
UBOOT_KERN_STARTADDR="0x80201000"

# Building the firmware image
X_BUILD_BUILD_IMG_DEFAULTS="mfsroot fsimage"
